<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?><?import javafx.scene.*?><?import javafx.scene.control.*?><?import javafx.scene.layout.*?><?import javafx.scene.shape.*?>

<VBox xmlns:fx="http://javafx.com/fxml/1" alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.12"
    fx:controller="trinque.eric.fractalGenerator.ui.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label fx:id="welcomeText" />
    <Circle fill="DODGERBLUE" onMouseEntered="#onHover" radius="100.0" stroke="BLACK" strokeType="INSIDE"
        fx:id="circleBlocker">
        <cursor>
            <Cursor fx:constant="OPEN_HAND" />
        </cursor>
    </Circle>
    <Button onAction="#onHelloButtonClick" text="Hello!" />
    <Button onAction="#onHelloButtonClick" text="You are here -&gt;" />
</VBox>
